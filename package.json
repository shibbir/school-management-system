{
  "name": "school-management-system",
  "version": "1.0.0",
  "description": "School Management System built with Node.js, React.js, PostgreSQL.",
  "scripts": {
    "start": "run-p webpack:development pm2:development",
    "webpack:development": "cross-env NODE_ENV=development webpack --progress --watch --stats normal",
    "pm2:development": "cross-env NODE_ENV=development pm2 start src/backend/config/server.js --watch --no-daemon --name school-management-system",
    "webpack:production": "cross-env NODE_ENV=production webpack --progress",
    "pm2:production": "cross-env NODE_ENV=production pm2 start src/backend/config/server.js --name school-management-system",
    "production": "run-s webpack:production pm2:production",
    "seed": "node src/backend/config/seeder.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Shibbir Ahmed",
  "license": "MIT",
